{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "data-analyzer.html",
      "use": "@vercel/static"
    },
    {
      "src": "datascraper.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/analyzer",
      "dest": "/data-analyzer.html"
    },
    {
      "src": "/api/scrape",
      "dest": "/datascraper.py"
    }
  ]
}
